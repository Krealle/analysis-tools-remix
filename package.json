{
  "name": "analysis-tools-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "build:css": "sass ./styles:./app/styles --style=compressed",
    "dev:css": "npm run build:css && sass --watch ./styles:./app/styles"
  },
  "dependencies": {
    "@remix-run/css-bundle": "2.11.1",
    "@remix-run/node": "2.11.1",
    "@remix-run/react": "2.11.1",
    "@remix-run/serve": "2.11.1",
    "@sinclair/typebox": "0.33.0",
    "@vercel/analytics": "1.3.1",
    "@vercel/remix": "2.11.1",
    "@vercel/speed-insights": "1.0.12",
    "ajv": "8.17.1",
    "graphql": "16.9.0",
    "graphql-request": "7.1.0",
    "isbot": "5.1.14",
    "luxon": "3.5.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.26.0",
    "zod": "3.23.8",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@types/luxon": "3.4.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "sass": "^1.77.8",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
