{
  "name": "analysis-tools-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "build:css": "sass ./styles:./app/styles --style=compressed",
    "dev:css": "npm run build:css && sass --watch ./styles:./app/styles"
  },
  "dependencies": {
    "@remix-run/css-bundle": "2.9.1",
    "@remix-run/node": "2.9.1",
    "@remix-run/react": "2.9.1",
    "@remix-run/serve": "2.9.1",
    "@sinclair/typebox": "0.32.5",
    "@vercel/analytics": "1.1.1",
    "@vercel/remix": "2.9.1",
    "@vercel/speed-insights": "1.0.2",
    "ajv": "8.12.0",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "isbot": "3.6.8",
    "luxon": "3.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.0",
    "zod": "3.22.4",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@types/luxon": "3.3.7",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "sass": "^1.69.5",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
